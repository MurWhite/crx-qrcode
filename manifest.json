{
    "name": "QRCode",
    "version": "1.0",
    "description": "快速页面查看对应的二维码!",
    "manifest_version": 2,
    "browser_action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "asset/logo-qrcode-16.png",
            "32": "asset/logo-qrcode-32.png",
            "48": "asset/logo-qrcode-48.png",
            "128": "asset/logo-qrcode-128.png"
        }
    },
    "icons": {
        "16": "asset/logo-qrcode-16.png",
        "32": "asset/logo-qrcode-32.png",
        "48": "asset/logo-qrcode-48.png",
        "128": "asset/logo-qrcode-128.png"
    },
    "background": {
        "scripts": ["background.js"]
    }
}